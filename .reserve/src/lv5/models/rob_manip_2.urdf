<?xml version="1.0"?>

<robot name="rob_manip_2">
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <material name="silver">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
            <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.05" radius="0.1"/>
            </geometry>
            <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <inertia ixx="0.0027" iyy="0.0027" izz="0.005" ixy="0" ixz="0" iyz="0"/>
            <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
        </inertial>
    </link>

    <link name="world"/>
        <joint name="fixed" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <link name="torso">
        <visual>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <material name="silver">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
            <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.1" radius="0.05"/>
            </geometry>
            <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <inertia ixx="0.00145" iyy="0.00145" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
            <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
        </inertial>
    </link>

    <joint name="hip" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="torso"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.05"/>
    </joint>

    <link name="upper_arm">
        <visual>
            <geometry>
                <cylinder length="0.4" radius="0.05"/>
            </geometry>
            <material name="silver">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
            <origin rpy="0.0 0 0" xyz="0 0 0.2"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.4" radius="0.05"/>
            </geometry>
            <origin rpy="0.0 0 0" xyz="0 0 0.2"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
            <origin rpy="0.0 0 0" xyz="0 0 0.2"/>
        </inertial>
    </link>

    <joint name="shoulder" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="torso"/>
        <child link="upper_arm"/>
        <origin rpy="0 0 0" xyz="0 0.1 0.05"/>
    </joint>

    <link name="lower_arm">
    <visual>
    <geometry>
    <cylinder length="0.2" radius="0.05"/>
    </geometry>
    <material name="silver">
    <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </visual>
    <collision>
    <geometry>
    <cylinder length="0.2" radius="0.05"/>
    </geometry>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </collision>
    <inertial>
    <mass value="1.0"/>
    <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </inertial>
    </link>

    <link name="lower_arm_2">
    <visual>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <material name="silver">
    <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </visual>
    <collision>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </collision>
    <inertial>
    <mass value="0.5"/>
    <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </inertial>
    </link>

    <link name="lower_arm_3">
    <visual>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <material name="silver">
    <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </visual>
    <collision>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </collision>
    <inertial>
    <mass value="0.5"/>
    <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </inertial>
    </link>

    <link name="lower_arm_4">
    <visual>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <material name="silver">
    <color rgba="0.75 0.75 0.75 1"/>
    </material>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </visual>
    <collision>
    <geometry>
    <cylinder length="0.1" radius="0.05"/>
    </geometry>
    <origin rpy="0.0 0 0" xyz="0 0 0.05"/>
    </collision>
    <inertial>
    <mass value="0.5"/>
    <inertia ixx="0.01396" iyy="0.01396" izz="0.00125" ixy="0" ixz="0" iyz="0"/>
    <origin rpy="0.0 0 0" xyz="0 0 0.1"/>
    </inertial>
    </link>

    <joint name="elbow" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="upper_arm"/>
        <child link="lower_arm"/>
        <origin rpy="0 0 0" xyz="0.0 -0.1 0.35"/>
    </joint>

    <joint name="elbow_2" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="lower_arm"/>
    <child link="lower_arm_2"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
    </joint>
    <joint name="elbow_3" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="lower_arm_2"/>
    <child link="lower_arm_3"/>
    <origin rpy="0 0 0" xyz="0.0 0.1 0.05"/>
    </joint>
    <joint name="elbow_4" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="lower_arm_3"/>
    <child link="lower_arm_4"/>
    <origin rpy="0 0 0" xyz="0.0 -0.1 0.1"/>
    </joint>
    <joint name="wrist" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="lower_arm_4"/>
    <child link="hand"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.1"/>
    </joint>

    <link name="hand">
    <visual>
        <geometry>
            <box size="0.05 0.05 0.05"/>
        </geometry>
        <material name="silver">
            <color rgba="0.75 0.75 0.75 1"/>
        </material>
        <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
    </visual>
    <collision>
        <geometry>
            <box size="0.05 0.05 0.05"/>
        </geometry>
        <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
    </collision>
    <inertial>
        <mass value="1.0"/>
        <inertia ixx="0.00042" iyy="0.00042" izz="0.00042" ixy="0" ixz="0" iyz="0"/>
        <origin rpy="0.0 0 0" xyz="0 0 0.025"/>
    </inertial>
    </link>

    <!-- <joint name="wrist" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="lower_arm"/>
        <child link="hand"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 0.4"/>
    </joint> -->

    <transmission name="tran0">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="hip">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor0">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran1">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="shoulder">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor1">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran2">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor2">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran3">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow_2">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor3">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran4">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow_3">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor4">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran5">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="elbow_4">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor5">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <transmission name="tran6">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="wrist">
            <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
        </joint>
        <actuator name="motor6">
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <gazebo reference="base_link">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="torso">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="upper_arm">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="lower_arm">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="lower_arm_2">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="lower_arm_3">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="lower_arm_4">
        <material>Gazebo/Chrome</material>
    </gazebo>
    <gazebo reference="hand">
        <material>Gazebo/Chrome</material>
    </gazebo>

    <gazebo>
        <plugin name="control" filename="libgazebo_ros_control.so">
        <robotNamespace></robotNamespace>
        </plugin>
    </gazebo>

    <gazebo>
        <plugin name="joint_state_publisher" filename= "libgazebo_ros_joint_state_publisher.so">
        <jointName>hip, shoulder, elbow, elbow_2, elbow_3, elbow_4, wrist</jointName>
        </plugin>
    </gazebo>

</robot>